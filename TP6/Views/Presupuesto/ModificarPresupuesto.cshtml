@{
    ViewData["Title"] = "ModificarPresupuesto";
}

@model Models.Presupuesto

<form asp-controller="Presupuesto" asp-action="ModificarPresupuesto">
  <div class="mb-3">
    <label asp-for=@Model.NombreDestinatario class="form-label">NombreDestinatario</label>
    <input asp-for=@Model.NombreDestinatario type="text" class="form-control">
    <label asp-for=@Model.FechaCreacion class="form-label">FechaCreacion</label>
    <input asp-for=@Model.FechaCreacion type="date" class="form-control">
  </div>    
  <button type="submit" class="btn btn-outline-success">Actualizar</button>
</form>
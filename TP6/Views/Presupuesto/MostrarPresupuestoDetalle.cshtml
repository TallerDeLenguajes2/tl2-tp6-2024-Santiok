@{
  ViewData["Title"] = "MostrarPresupuestoDetalle";
}


@model List<Models.PresupuestosDetalles>

<div class="text-center">
  <h1 class="display-4">Listado de presupuesto detalle de Producto</h1>
</div>
<table class="table table-primary table-striped">
  <thead>
    <tr>
      <th scope="col">ID Producto</th>
      <th scope="col">Descripcion</th>
      <th scope="col">Precio</th>
      <th scope="col">Cantidad</th>
    </tr>
  </thead>
  <tbody>
    @foreach (var presupuestoDetalle in Model)
    {
      <tr>
        <th scope="row">@presupuestoDetalle.Producto.IdProducto</th>
        <td>@presupuestoDetalle.Producto.Descripcion</td>
        <td>@presupuestoDetalle.Producto.Precio</td>
        <td>@presupuestoDetalle.Cantidad</td>
      </tr>
    }
  </tbody>
</table>
